class Solution {
    public boolean isPalindrome(int x) {
        String str = String.valueOf(x);
        String temp = "";
        for(int i = str.length()-1; i >= 0; i--){
            temp += str.charAt(i);
        }
        if(temp.equals(str)){
            return true;
        }else{
            return false;
        }
    }
}
